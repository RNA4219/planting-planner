{
  "ci": {
    "collect": {
      "numberOfRuns": 1,
      "settings": {
        "preset": "mobile",
        "formFactor": "mobile"
      },
      "url": [
        "http://localhost:3000/"
      ]
    },
    "assert": {
      "assertions": {
        "categories:pwa": [
          "error",
          {
            "minScore": 0.9
          }
        ],
        "categories:performance": [
          "warn",
          {
            "minScore": 0.8
          }
        ],
        "categories:accessibility": [
          "error",
          {
            "minScore": 0.9
          }
        ]
      }
    }
  }
}
